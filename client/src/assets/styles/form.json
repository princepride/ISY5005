            "title": "radioButton",
            "items": {
                "type": "string",
                "enumTitle": [
                "1","2","3","4","5","6","7","8"
                ]
            }
        },
        "checkbox": {
            "type": "checkBoxes",
            "title": "checkbox",
            "items": {
                "type": "string",
                "enumTitle": [
                "1","2","3","4","5","6","7","8"
                ]
            }
