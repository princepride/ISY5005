{
    "title": "Spa Booking",
    "required" : [
        "name",
        "phoneNumber",
        "beginTime"
    ],
    "properties": {
        "name": {
            "type": "textInput",
            "title": "Name",
            "default": "Tom"
        },
        "phoneNumber": {
            "type": "textInput",
            "title": "Phone Number"
        },
        "Time": {
            "type": "time",
            "title": "Begin Time"
        },
        "Date": {
            "type": "date",
            "title": "Begin Date"
        },
        "serviceCategories": {
            "type": "dropdownList",
            "title": "Service categories",
            "items": {
                "type": "string",
                "enumTitle": [
                "1","2","3","4","5","6","7","8"
                ]
            }
        },
        "peopleService": {
            "type": "radiButtons",
            "title": "peopleService",
            "items": {
                "type": "string",
                "enumTitle": [
                "Emily Johnson","Benjamin Green","Olivia Taylor","Lucas Parker","William Brown"
                ]
            }
        },
        "note": {
            "type": "textarea",
            "title": "Note"
        },
        "button": {
            "type": "button",
            "title": "submit"
        }
    }
}